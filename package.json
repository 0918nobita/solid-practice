  "private": true,
  "scripts": {
    "typecheck": "tsc -w",
    "build": "ts-node --project ./scripts/tsconfig.json ./scripts/build.ts",
    "serve": "ts-node --project ./scripts/tsconfig.json ./scripts/serve.ts",
    "watch": "ts-node --project ./scripts/tsconfig.json ./scripts/watch.ts",
    "dev": "run-p typecheck watch serve",
    "lint": "eslint './**/*.{ts,tsx,js}'",
    "format": "eslint --fix './**/*.{ts,tsx,js}'"
  },
  "dependencies": {
    "solid-js": "^0.20.2"
  },
  "devDependencies": {
    "@types/node-static": "^0.7.5",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "chokidar": "^3.4.2",
    "esbuild": "^0.7.8",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "node-static": "^0.7.11",
    "npm-run-all": "^4.1.5",
    "open": "^7.3.0",
    "prettier": "^2.1.2",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  }
}
